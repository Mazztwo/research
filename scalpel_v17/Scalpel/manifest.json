{
	"name": "Scalpel",
	"description": "On page load, outputs the DOM and HTTP requests to the console.",
	"version": "1.1",

	"permissions":
	[
		"webRequest", "webNavigation", "<all_urls>"
	],

	"content_scripts":
	[
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],

	"background":
	{
		"scripts": ["background.js"]
	},

	"manifest_version": 2
}
